<div class="container">
    <h3> Cписок гостей </h3>
    <table class="table">
        <thead>
            <tr>
                <th>№</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
                <th>Описание</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.Id</td>
                <td>@item.SurName</td>
                <td>@item.FirstName</td>
                <td>@item.Partronymic</td>
                <td>@item.Age</td>
                <td>@Html.ActionLink("Детали","Details", new { id = item.Id })</td>
            </tr>
            }
        </tbody>
    </table>
    <p>
        <a asp-controller="Home" asp-action="Index">Назад</a>
    </p>
</div>
    

